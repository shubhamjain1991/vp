<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="{http://java.sun.com/xml/ns/persistence} {http://java.sun.com/xml/ns/persistence_2_0.xsd}"
	version="2.0">

	<persistence-unit name="spring-jpa-unit">
	<provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
	<class>com.vp.enity.CustomerProfile</class>
	<class>com.vp.enity.AddressDetails</class>
	<class>com.vp.enity.ApplicationForm</class>
	<class>com.vp.enity.ServiceProviders</class>
	<class>com.vp.enity.MessageCode</class>
	</persistence-unit>

</persistence>